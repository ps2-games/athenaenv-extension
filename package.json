{
  "name": "athenaenv-extension",
  "displayName": "AthenaEnv Extension (JS)",
  "description": "Allows trailing 'f' on decimal numbers in JavaScript (C#-style floats) when workspace has athena.ini.",
  "version": "0.0.2",
  "publisher": "CarterTMDev",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": ["Linters", "Programming Languages"],
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:athena.ini"
  ],
  "extensionKind": ["workspace"],
  "main": "./src/extension.js",
  "contributes": {
    "grammars": [
      {
        "scopeName": "athena.floats.injection",
        "path": "./syntaxes/athena-float-injection.json",
        "injectTo": ["source.js", "source.jsx"],
        "priority": "low"
      }
    ],
    "typescriptServerPlugins": [
      {
        "name": "athenaenv-extension-plugin",
        "enableForWorkspaceTypeScriptVersions": true,
        "languages": ["javascript", "javascriptreact"]
      }
    ]
  },
  "files": [
    "src",
    "node_modules/athenaenv-extension-plugin",
    "syntaxes",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CarterTMDev/athenaenv-extension.git"
  },
  "scripts": {
    "build": "vsce package -o ./builds/"
  }
}